<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
   
    <RootNamespace>LiveScore.Soccer</RootNamespace>
    <AssemblyName>LiveScore.Soccer</AssemblyName>
    <Configurations>Debug;Release;Test;AutoTest;DevRelease</Configurations>
  </PropertyGroup>
  <PropertyGroup>
    <DebugType>pdbonly</DebugType>
    <DesignTimeBuild Condition="'$(NCrunch)' == '1'">true</DesignTimeBuild>
    <RunAnalyzersDuringLiveAnalysis>false</RunAnalyzersDuringLiveAnalysis>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <CodeAnalysisRuleSet>Soccer.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='DevRelease|AnyCPU'">
    <CodeAnalysisRuleSet>Soccer.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='AutoTest|AnyCPU'">
    <CodeAnalysisRuleSet>Soccer.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <CodeAnalysisRuleSet>Soccer.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Test|AnyCPU'">
    <CodeAnalysisRuleSet>Soccer.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Models\Odds\" />
    <Folder Include="Views\Templates\MatchDetails\DetailTracker\" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Views\SoccerMatchListViewTemplate.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Models\Odds\OddsEventsMessage.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="ViewResources\MatchDetailH2HViewResource.xaml.cs">
      <DependentUpon>MatchDetailH2HViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\MatchDetailStatisticViewResource.xaml.cs">
      <DependentUpon>MatchDetailStatisticViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\MatchDetailLinesUpViewResource.xaml.cs">
      <DependentUpon>MatchDetailLinesUpViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\MatchDetailTrackerViewResource.xaml.cs">
      <DependentUpon>MatchDetailTrackerViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\MatchDetailInfoViewResource.xaml.cs">
      <DependentUpon>MatchDetailInfoViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\MatchDetailOddsViewResource.xaml.cs">
      <DependentUpon>MatchDetailOddsViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\SoccerViewResource.xaml.cs">
      <DependentUpon>SoccerViewResource.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewResources\MatchDetailViewResource.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
    <Compile Update="Views\MatchDetailView.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Views\SoccerMatchListViewTemplate.xaml.cs">
      <DependentUpon>MatchDataTemplate.xaml</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailInfo\InfoItems\PenaltyShootOutTemplate.xaml.cs">
      <DependentUpon>PenaltyShootOutTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailInfo\InfoItems\ScoreChangeItemTemplate.xaml.cs">
      <DependentUpon>ScoreChangeItemTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailInfo\InfoItems\DefaultItemTemplate.xaml.cs">
      <DependentUpon>DefaultItemTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailInfo\InfoItems\MainEventItemTemplate.xaml.cs">
      <DependentUpon>MainEventItemTemplate.xaml</DependentUpon>
      <ExcludeFromStyleCop>false</ExcludeFromStyleCop>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailInfo\InfoTemplate.xaml.cs">
      <DependentUpon>InfoTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailOdds\OddsTemplate.xaml.cs">
      <DependentUpon>OddsTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MatchDetailInfoTemplate.xaml.cs">
      <DependentUpon>InfoTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Views\Templates\MatchDetailInfo\ScoreChangeItemTemplate %28copy%29.xaml.cs">
      <DependentUpon>PenaltyMissedItemTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Views\Templates\MatchDataTemplate.xaml.cs">
      <DependentUpon>MatchDataTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MatchDetails\DetailOdds\OddsMovementView.xaml.cs">
      <DependentUpon>OddsMovementView.xaml</DependentUpon>
    </Compile>
   

    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Views\Templates\MatchDetails\DetailOdds\OddsItems\H2HTemplate.xaml.cs">
      <DependentUpon>H2HTemplate.xaml</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Fanex.Caching" Version="2.2.1" />
    <PackageReference Include="MethodTimer.Fody" Version="3.1.0" />
    <PackageReference Include="MessagePack" Version="1.7.3.7" />
    <PackageReference Include="MessagePackAnalyzer" Version="1.7.3.7" />
    <PackageReference Include="Prism.DryIoc.Forms" Version="7.2.0.1367" />
    <PackageReference Include="Refit" Version="4.7.51" />
    <PackageReference Include="SonarAnalyzer.CSharp" Version="7.16.0.8981">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Xamarin.Forms" Version="4.2.0.709249" />
    <PackageReference Include="PropertyChanged.Fody" Version="3.1.3" />
    <PackageReference Include="AsyncErrorHandler.Fody" Version="1.3.0" />
    <PackageReference Include="XamEffects" Version="1.6.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\Shared\Common\LiveScore.Common.csproj" />
    <ProjectReference Include="..\..\Core\LiveScore.Core.csproj" />
  </ItemGroup>
