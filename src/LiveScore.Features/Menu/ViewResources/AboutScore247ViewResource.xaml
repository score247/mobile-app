<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary x:Class="LiveScore.Features.Menu.ViewResources.AboutScore247ViewResource"
                    xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:ffImageLoading="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Style x:Key="AppLogo"
           TargetType="{x:Type ffImageLoading:CachedImage}">
        <Setter Property="WidthRequest"
                Value="100" />
        <Setter Property="Margin"
                Value="0,64,0,32" />
    </Style>

    <Style x:Key="VerionApp" TargetType="Label">
        <Setter Property="HorizontalTextAlignment"
                Value="Center" />
        <Setter Property="TextColor"
                Value="{StaticResource SettingColor}" />
        <Setter Property="FontSize"
                Value="{StaticResource SettingFontSize}" />
    </Style>
</ResourceDictionary>