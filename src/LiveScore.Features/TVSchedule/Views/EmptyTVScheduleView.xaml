<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
        x:Class="LiveScore.Features.TVSchedule.Views.EmptyTVScheduleView"
        xmlns="http://xamarin.com/schemas/2014/forms"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        xmlns:mvvm="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
        xmlns:coreResources="clr-namespace:LiveScore.Core.ViewResources;assembly=LiveScore.Core"
        xmlns:langs="clr-namespace:LiveScore.Common.LangResources;assembly=LiveScore.Common"
        xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
        xmlns:coreTemplates="clr-namespace:LiveScore.Core.Views.Templates;assembly=LiveScore.Core"
        Title="{Binding Title}"
        x:Name="self"
        prism:ViewModelLocator.AutowireViewModel="True"
        BackgroundColor="{StaticResource PrimaryColor}">
    <NavigationPage.TitleView>
        <coreTemplates:NavigationTitleTemplate
                    Title="{langs:Translate TV}"
                    mvvm:ViewModelLocator.AutowirePartialView="{x:Reference self}" />
    </NavigationPage.TitleView>
    <ContentPage.ToolbarItems>
        <ToolbarItem IconImageSource="images/common/icon_transparent.png"
                     Priority="1"
                     Text="Search" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <Label Text="TV Schedule is comming soon"
           TextColor="{StaticResource SecondTextColor}"
           HorizontalOptions="Center"
           VerticalOptions="Center" FontSize="13" />
    </ContentPage.Content>
</ContentPage>